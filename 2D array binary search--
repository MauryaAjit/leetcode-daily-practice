import java.util.*;

public class Main {
    public static void main(String[] args) {
      int [][]arr= {{2,4,6,9},{10,12,13,14},{15,17,20,22},{24,26,28,30}};
      int target= 17;
      int Rrow=-1;
      int Rcol=-1;
      for(int i=0; i< arr.length;i++){
        for( int j=0;j<arr[i].length;j++){
          if(arr[i][j]==target){
            Rrow=i;
            Rcol=j;
            break;
          }
        }
      }
      System.out.println(Rrow);
      System.out.println(Rcol);
  }
}
